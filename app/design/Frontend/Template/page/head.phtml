<?php $js = $this->getJs(); ?>
<?php if (count($js)): ?>
    <?php foreach ($js as $_js): ?>
        <script src="<?php echo $_js; ?>"></script>
    <?php endforeach; ?>
<?php endif; ?>

<?php $css = $this->getCss(); ?>
<?php if (count($css)): ?>
    <?php foreach ($css as $_css): ?>
        <link href="<?php echo $_css ?>" rel="stylesheet">
    <?php endforeach; ?>
<?php endif; ?>